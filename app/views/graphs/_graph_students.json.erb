        {
		  <% student_records = Graph.find(params[:graph_id]).course.students.map{ |rec| [rec.last.capitalize, rec.first.capitalize, rec.id] } %>
          <% student_records.sort! %>
          <% for i in 0...student_records.length - 1 %>
		  <% rec = student_records[i] %>
          "<%= rec[0] + ", " + rec[1] %>" : "<%= rec[2] %>",
          <% end %>
		  "<%= student_records[student_records.length - 1][0] + ", " + student_records[student_records.length - 1][1] %>" : "<%= student_records[student_records.length - 1][2] %>"
        }
